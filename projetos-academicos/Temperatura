#include <stdio.h>

int main() {
    float temperatura[8];
    int i;
    int hora = 9;

    for (i = 0; i < 8; i++) {
        printf("digite a temperatura  %d: ", hora);
        scanf("%f", &temperatura[i]);
        hora++;
    }
    printf("\ntemperaturas registradas:\n");
    for (i = 0; i < 8; i++) {
        printf("hora %d: %.2fï¿½C\n", 9 + i, temperatura[i]);
    }

    return 0;
}
